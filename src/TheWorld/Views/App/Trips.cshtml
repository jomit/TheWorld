@{
    ViewBag.Title = "My Trips";
}
@section Scripts{
    <script src="~/lib/underscore/underscore-min.js"></script>
    <script src="~/lib/angular/angular.min.js" type="text/javascript"></script>
    <script src="~/lib/angular-route/angular-route.min.js" type="text/javascript"></script>
    <script src="~/lib/travelmap/travelmap.min.js" type="text/javascript"></script>
    <script src="http://maps.google.com/maps/api/js?sensor=true" type="text/javascript"></script>
    <script src="~/lib/gmaps/gmaps.min.js" type="text/javascript"></script>
    <environment names="Development">
        <script src="~/js/simpleControls.js" type="text/javascript"></script>
        <script src="~/js/app-trips.js" type="text/javascript"></script>
        <script src="~/js/tripsController.js" type="text/javascript"></script>
        <script src="~/js/tripEditorController.js" type="text/javascript"></script>
    </environment>
    <environment names="Production, Staging">
        <script src="~/lib/_app/simpleControls.js" type="text/javascript"></script>
        <script src="~/lib/_app/app-trips.js" type="text/javascript"></script>
        <script src="~/lib/_app/tripsController.js" type="text/javascript"></script>
        <script src="~/lib/_app/tripEditorController.js" type="text/javascript"></script>
    </environment>
    
}
<div class="row" data-ng-app="app-trips">
    <div data-ng-view></div>
</div>
